{
    "version": 2,
    "builds": [
        {
            "src": "backend/index.ts",
            "use": "@vercel/node"
        },
        {
            "src": "frontend/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "backend/index.ts"
        },
        {
            "src": "/(.*)",
            "dest": "frontend/$1"
        }
    ]
}